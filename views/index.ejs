<%-include('cabecalho.ejs')%>
    <table>
        <tr>
            <th>ID</th>
            <th>Nome</th>
            <th>E-mail</th>
            <th>Foto</th>
            <th>Deletar</th>
            <th>Editar</th>
        </tr>
        <%for(usuario of Usuarios){%>
        <tr>
            <td><%=usuario._id%></td>
            <td><%=usuario.nome%></td>
            <td><%=usuario.email%></td>
            <td><img class="foto" src="/fotos/<%=usuario.foto%>"/></td>
            <td>
                <a href="/del/<%=usuario._id%>">Deletar</a>
            </td>
            <td>
                <a href="/edt/<%=usuario._id%>">Editar</a>
            </td>
        </tr>
        <% } %>
    </table>
<%-include('rodape.ejs')%>
